pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building app Carlos'
            }
        }
        
        stage('Developing') {
            steps {
                echo 'Developing app Carlos'
            }
        }
        
        stage('Sleeping') {
            steps {
                sleep 30
                echo 'Sleeping'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing app Carlos'
            }
        }
    }
}

